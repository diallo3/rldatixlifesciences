                                </svg>
                            </div>

                        </div>
                    
                        <div class="relative max-w-sm mx-auto space-y-12 md:max-w-7xl inview-container">
                            {# title #}
                            <div class="text-center feature-with-key-list__title inview-item">
                                {{ headings.headingThree({
                                    title: keyList.title,
                                    size: 'fluid-text-4xl',
                                })}}
                            </div>
                            {# entry #}
                            <div class="max-w-3xl mx-auto mt-16 lg:max-w-7xl">
                                <dl class="grid gap-4 md:grid-cols-2 lg:grid lg:grid-cols-4">
                                    {% for list in keyList.list %}
                                    <dd class="flex items-start space-x-4 inview-item">
                                        <div class="p-2 border border-dodger-blue-600 text-dodger-blue-600">{{ icons.icon('brand-glyph', 'size-4 ') }}</div>
                                        <span class="inline-flex text-lg font-semibold">{{ list.item }}</span>
                                    </dd>
                                    {% endfor %}
                                </dl>
                            </div>
                            {# cta #}
                            <div class="relative flex justify-center feature-with-key-list__cta">

                                <div class="max-w-4xl mx-auto space-y-8 text-center feature-with-key-list__cta-entry">
                                    {# <div class="prose prose-invert prose-p:fluid-text-base">
                                        {{ keyList.entry}}
                                    </div> #}
                                
                                    <div class="text-center inview-item">
                                        {{ buttons.btnLink({
                                            text: 'Tour the Platform',
                                            link: keyList.link.title,
                                            size: 'xl',
                                            icon: 'arrow-right',
                                        })}}
                                    </div>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
    {% endblock %}
{% endembed %}