                {#  container #}
                <div class="mx-auto space-y-12 feature-with-key-list__header">

                    <div class="feature-with-key-list__entry">
                        <div class="absolute flex items-center h-full inset-y-16 -inset-x-20">
                            <div class="relative flex items-center justify-center hidden w-full opacity-75">
                                {{ images.imageObject({
